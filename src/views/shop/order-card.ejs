<%- include('../_includes/head') %>

<%- include('../_includes/main-nav') %>

<header class="bg-white shadow">
    <div class="max-w-7xl mx-auto py-6 px-4 sm:px-6 lg:px-8">
        <h1 class="text-3xl font-bold text-gray-900">
            <%= pageTitle %>
            <%= order.createdAt.getDate() %>/<%= order.createdAt.getMonth() %>/<%= order.createdAt.getFullYear() %> <%= order.createdAt.getHours() %>:<%= order.createdAt.getMinutes() %>:<%= order.createdAt.getSeconds() %>
        </h1>
    </div>
</header>
<main>
    <div class="max-w-7xl mx-auto py-6 sm:px-6 lg:px-8">
        <div class="px-4 py-6 sm:px-0">
            <% if (order) { %>
                <div class="flex">
                    Content
                </div>
            <% } else { %>
                <p>Order doesn't exist</p>
            <% } %>
        </div>
    </div>
</main>

<%- include('../_includes/footer') %>
